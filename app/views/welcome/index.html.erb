<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
<script src = '/app/assets/javascripts/application.js'> </script>
<div class= "row">
    <div class="col s5 buff">
      <div class="video-container">
        <iframe src="http://www.youtube.com/embed/<%= @youtube.first['youtubeid']%>" ></iframe>
      </div>
      <% @youtube[1..2].each do |x| %>
        <div class="video-container" style="max-width:60%; max-height:50%; margin: 15px 0px;">
          <iframe src="http://www.youtube.com/embed/<%= x['youtubeid']%>" ></iframe>
        </div>
      <% end %>
    </div>

    <div class="col s3">
    </br>
    <div id="col2">
      <h1 id="headlines" class="z-depth-2">HEADLINES</h1>
        <div id= "wrapper">
          <%@bing.each do |x| %>
          <a class="headline" href = "<%=x[:url]%>" > <%=x['title']%> </a>
              <p class="body-ar"><%= x['source']%></p>
              <p class="body-ar"><%= x['description']%></p>
              <p class="body-ar"><%= x['date']%></p>
          </p>
          <%end%>
        </div>
      </div>
    </div>
<div class="col s3">
  <div class="phatty"></div>
    <h1 class= "section">#TRENDING</h1>
      <div id="tweetbox">
        <ul>
          <% @twit.each do |x| %>
          <li>
        <a class="helvetica-bold tweetlink" href = "<%=x[:url]%>" > <%=x[:name]%> </a>
          </li>
          <% end %>
        </ul>
    </div>
  </div>
</div>
</div>
<div class= "row">
    <div class="col s6">
      <div class="phatty"></div>
        <h1 class= "section">NEW TRACKS</h1>
      <p>
        <% @soundc.each do |x| %>
          <%= x['embedhtml'].html_safe %>
        <% end %>
      </p>
  </div>
  <div class="col s6">
    <% @instagram[0..11].each do |x|%>
      <img class="materialboxed" width="200" height="200" src='<%= x['url'] %>'></img>
    <% end %>
  </div>
</div>
